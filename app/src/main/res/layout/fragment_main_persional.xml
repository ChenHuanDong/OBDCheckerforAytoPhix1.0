<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true"
            android:background="@color/colorOBDbackground"
            tools:context="com.example.administrator.obdcheckerforaytophix10.main.MainPersionalFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <!--上方标题栏-->
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x40">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/personal"
                android:textColor="@color/colorDashboardsPointer"
                android:textSize="@dimen/x18"/>


            <TextView
                android:id="@+id/tv_personal_menu"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/x319"
                android:gravity="center_vertical"
                android:text="@string/menu"
                android:textColor="@color/colorTextColorDemo"/>

        </RelativeLayout>
        <!--Line-->
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x1"
            android:src="@drawable/line_ui"/>

        <!--Name-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x34"
            android:text="@string/name"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <EditText
                android:layout_width="@dimen/x250"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x15"
                android:gravity="center_vertical"
                android:hint="@string/name"
                android:textColor="@color/colorTextColorDemo"
                android:textColorHint="@color/colorOBDbackground"
                android:textSize="@dimen/x16"/>

        </RelativeLayout>

        <!--Vehicle identification-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x30"
            android:text="@string/vehicleidentificationnumber"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/identificationdemo"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>

        </RelativeLayout>

        <!--Year-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x34"
            android:text="@string/year"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:id="@+id/re_personal_year"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/onenineninesix"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>

            <!--Select-->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/x15"
                android:gravity="center_vertical|right"
                android:text="@string/select"
                android:textColor="@color/colorDashboardsPointer"
                android:textSize="@dimen/x16"/>

        </RelativeLayout>

        <!--MAKE-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x34"
            android:text="@string/make"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:id="@+id/re_personal_make"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/ac"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>

            <!--Select-->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/x15"
                android:gravity="center_vertical|right"
                android:text="@string/select"
                android:textColor="@color/colorDashboardsPointer"
                android:textSize="@dimen/x16"/>

        </RelativeLayout>


        <!--MODEL-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x34"
            android:text="@string/model"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:id="@+id/re_personal_model"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/cobra"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>

            <!--Select-->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/x15"
                android:gravity="center_vertical|right"
                android:text="@string/select"
                android:textColor="@color/colorDashboardsPointer"
                android:textSize="@dimen/x16"/>

        </RelativeLayout>


        <!--OPTION-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x34"
            android:text="@string/option"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <EditText
                android:layout_width="@dimen/x250"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:hint="@string/optiontatil"
                android:textColor="@color/colorTextColorDemo"
                android:textColorHint="@color/colorOBDbackground"
                android:textSize="@dimen/x16"/>

            <!--Select-->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/x15"
                android:gravity="center_vertical|right"
                android:text="@string/select"
                android:textColor="@color/colorDashboardsPointer"
                android:textSize="@dimen/x16"/>

        </RelativeLayout>


        <!--TYPE-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x34"
            android:text="@string/type"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:id="@+id/re_personal_type"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/sedan"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>

            <!--Select-->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/x35"
                android:gravity="center_vertical|right"
                android:text="@string/select"
                android:textColor="@color/colorDashboardsPointer"
                android:textSize="@dimen/x16"/>

            <ImageView
                android:layout_width="@dimen/x10"
                android:layout_height="@dimen/x18"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x350"
                android:scaleType="fitXY"
                android:src="@drawable/enter"/>


        </RelativeLayout>


        <!--FUEL TYPE-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x34"
            android:text="@string/fueltype"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/select"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/x35"
                android:gravity="center_vertical|right"
                android:text="@string/gasolinepertrol"
                android:textColor="@color/colorDashboardsPointer"
                android:textSize="@dimen/x16"/>

            <ImageView
                android:layout_width="@dimen/x10"
                android:layout_height="@dimen/x18"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x350"
                android:scaleType="fitXY"
                android:src="@drawable/enter"/>


        </RelativeLayout>

        <!--ENGINE SIZE (LITRES)-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x34"
            android:text="@string/enginesizelitres"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/twonumber"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>


            <ImageView
                android:layout_width="@dimen/x22"
                android:layout_height="@dimen/x22"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x333"
                android:scaleType="fitXY"
                android:src="@drawable/sigh"/>

        </RelativeLayout>


        <!--VOLUMETRIC EFFICIENCY(%)-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x34"
            android:text="@string/volumetricefficiency"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/sixfive"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>


            <ImageView
                android:layout_width="@dimen/x22"
                android:layout_height="@dimen/x22"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x333"
                android:scaleType="fitXY"
                android:src="@drawable/sigh"/>

        </RelativeLayout>

        <!--BRAKE SPECIFIC FUEL CONSUMPTION(LB/HP·HR)-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x34"
            android:text="@string/brakespecificfuelconsumption"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/zeropointfourfive"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>


            <ImageView
                android:layout_width="@dimen/x22"
                android:layout_height="@dimen/x22"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x333"
                android:scaleType="fitXY"
                android:src="@drawable/sigh"/>

        </RelativeLayout>

        <!--FUEL CALCULATION METHOD-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x34"
            android:text="@string/fuelcalculationmethod"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/select"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/x35"
                android:gravity="center_vertical|right"
                android:text="@string/massairflowrate"
                android:textColor="@color/colorDashboardsPointer"
                android:textSize="@dimen/x16"/>

            <ImageView
                android:layout_width="@dimen/x10"
                android:layout_height="@dimen/x18"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x350"
                android:scaleType="fitXY"
                android:src="@drawable/enter"/>


        </RelativeLayout>

        <!--FUEL TANK CAPACITY (GAL)-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x30"
            android:text="@string/fueltankcapacitygal"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/onefour"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>

        </RelativeLayout>

        <!--FUEL COST PER UNIT ($)-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x30"
            android:text="@string/fuelcostperunit"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/fourdetail"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>

        </RelativeLayout>


        <!--VEHICLE SPEED SCALE FACTOR-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x25"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginTop="@dimen/x34"
            android:text="@string/vehiclespeedacalefactor"
            android:textColor="@color/colorTextColorDemo"
            android:textSize="@dimen/x14"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x44"
            android:background="@color/colorDialogBackGround">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center_vertical"
                android:text="@string/onedetail"
                android:textColor="@color/colorTextColorDemo"
                android:textSize="@dimen/x16"/>


            <ImageView
                android:layout_width="@dimen/x22"
                android:layout_height="@dimen/x22"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x333"
                android:scaleType="fitXY"
                android:src="@drawable/sigh"/>

        </RelativeLayout>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x27"
            android:background="@color/colorOBDbackground"/>


    </LinearLayout>
</ScrollView>
