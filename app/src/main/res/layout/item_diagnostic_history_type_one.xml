<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x60"
                android:background="@color/colorOBDbackground">


    <TextView
        android:id="@+id/tv_diagnostic_history_one_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x23"
        android:layout_marginLeft="@dimen/x20"
        android:layout_marginTop="@dimen/x36"
        android:singleLine="true"
        android:text="@string/ford"
        android:textAllCaps="true"
        android:textColor="@color/colorTextColorDemo"/>

    <TextView
        android:id="@+id/tv_diagnostic_history_one_content"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x23"
        android:layout_marginLeft="@dimen/x280"
        android:layout_marginTop="@dimen/x36"
        android:singleLine="true"
        android:textAllCaps="false"
        android:textColor="@color/colorTextColorDemo"/>


</RelativeLayout>