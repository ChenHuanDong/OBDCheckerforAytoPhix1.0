<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboards_main_out_re"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorOBDbackground"
    tools:context="com.example.administrator.obdcheckerforaytophix10.dashboards.OBDDashboardsActivity">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/x44"
        android:background="@color/colorOBDbackground"
        android:gravity="center"
        android:singleLine="true"
        android:text="@string/dashboards"
        android:textAllCaps="false"
        android:textColor="@color/colorConnectDetail"
        android:textSize="@dimen/x18"/>

    <!--返回按钮-->
    <ImageView
        android:layout_width="@dimen/x10"
        android:layout_height="@dimen/y18"
        android:layout_marginLeft="@dimen/x15"
        android:layout_marginTop="@dimen/y13"
        android:scaleType="fitXY"
        android:src="@drawable/return_ui"/>

    <!--返回按钮上面的透明按钮-->
    <ImageView
        android:id="@+id/dashboards_main_iv_return"
        android:layout_width="@dimen/x35"
        android:layout_height="@dimen/y31"
        android:background="#00000000"/>


    <!--菜单按钮-->
    <ImageView
        android:layout_width="@dimen/x20"
        android:layout_height="@dimen/y15"
        android:layout_marginLeft="@dimen/x340"
        android:layout_marginTop="@dimen/y15"
        android:scaleType="fitXY"
        android:src="@drawable/other_ui"/>

    <!--菜单按钮上面的透明按钮-->
    <ImageView
        android:id="@+id/dashboards_main_iv_other"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y30"
        android:layout_marginLeft="@dimen/x330"
        android:background="#00000000"/>


    <!-- 线 -->
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="@dimen/x1"
        android:layout_marginTop="@dimen/x45"
        android:scaleType="fitXY"
        android:src="@drawable/line_ui"/>


    <android.support.v4.view.ViewPager
        android:id="@+id/dashboards_main_vp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/y44"
        android:background="@color/colorOBDbackground"/>


    <LinearLayout
        android:id="@+id/dashboards_mian_bootom_ll"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y30"
        android:layout_marginTop="@dimen/y617"
        android:background="@color/colorOBDbackground"
        android:gravity="center"
        android:orientation="horizontal">


        <!--初始的点-->
        <com.example.administrator.obdcheckerforaytophix10.dashboards.dashboardsview.DashboardsMainSelecterPoint
            android:layout_width="@dimen/x19"
            android:layout_height="@dimen/y30"/>

        <com.example.administrator.obdcheckerforaytophix10.dashboards.dashboardsview.DashboardsMainPoint
            android:layout_width="@dimen/x19"
            android:layout_height="@dimen/y30"/>

        <com.example.administrator.obdcheckerforaytophix10.dashboards.dashboardsview.DashboardsMainPoint
            android:layout_width="@dimen/x19"
            android:layout_height="@dimen/y30"/>

    </LinearLayout>


</RelativeLayout>
