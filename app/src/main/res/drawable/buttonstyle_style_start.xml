<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true">
        <shape>
            <corners android:bottomLeftRadius="@dimen/x5"
                     android:topLeftRadius="@dimen/x5"/>
            <solid android:color="@color/colorTextColorDemo"/>
            <stroke android:width="@dimen/x1"
                    android:color="@color/colorTextColorDemo"/>
        </shape>
    </item>

    <item android:state_checked="false">
        <shape>
            <corners android:bottomLeftRadius="@dimen/x5"
                     android:topLeftRadius="@dimen/x5"/>
            <solid android:color="@color/colorOBDbackground"/>
            <stroke android:width="@dimen/x1"
                    android:color="@color/colorTextColorDemo"/>
        </shape>
    </item>

</selector>